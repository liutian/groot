{
  "extends": "../base.tsconfig.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "outDir": "dist",
    "jsx": "react-jsx",
    "sourceMap": true,
    "paths": {
      "@model/*": [
        "model/*"
      ],
      "@util/*": [
        "util/*"
      ],
      "@components/*": [
        "components/*"
      ]
    }
  },
  "include": [
    "./src"
  ]
}