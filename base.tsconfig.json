{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2020",
    "strict": true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "declaration": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "noImplicitAny": false
  }
}