{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2022",
    "lib": [
      "es2021"
    ],
    "strict": true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "declaration": true,
    "noUncheckedIndexedAccess": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false
  }
}